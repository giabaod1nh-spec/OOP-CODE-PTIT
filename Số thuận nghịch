#include<iostream>
using namespace std ; 
bool check(int n ){
  while(n > 0 ){
  int temp = 0 ; 
  int remainder ; 
  remainder = n % 10 ; 
  temp = temp * 10 + remainder ; 
  n /= 10 ; 
   } 
  if( temp == n ){
    return true ; 
  }
  else{
    return false ; 
    }
  }
int main(){
  int n ; cin >> n ; 
  cout<<check(int n) ; 
  }
  
